    "test-travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec $(find ./test -name \"*.spec.js\" -not -path \"./node_modules/*\")",
    "travis": "npm test"
  },
