{
  "name": "smartbot",
  "version": "0.1.0",
  "description": "SmartBot is a flexible and intelligent Slack bot for Node.JS",
  "dependencies": {
    "bole": "^2.0.0",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "smartslack": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3"
